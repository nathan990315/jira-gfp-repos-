jira.Filter filtro_calendario_@s {
  name = "filtro calendario "
  owner = {
    id = "61447938ff23ba007191345f"
    displayName = "Nathan Giovannini"
  }
  jql = "\"Data di esecuzione [Date]\" >= 0d AND \"Data di esecuzione [Date]\" <= 0d AND ${ jira.Field.instance.Project__project } in (PR, RG, HOT, CDV) AND ${ jira.Field.instance.Issue_Type__issuetype@suu } in standardIssueTypes() order by ${ jira.Field.instance.Created__datetime } DESC"
}
